all: simple-hole simple-chmod simple-ls simple-cat simple-ls2

simple-hole : simple-hole.c 
	gcc -o simple-hole simple-hole.c -I.

simple-chmod: simple-chmod.c
	gcc -o simple-chmod simple-chmod.c

simple-ls: simple-ls.c
	gcc -o simple-ls simple-ls.c

simple-cat: simple-cat.c
	gcc -o simple-cat simple-cat.c

simple-ls2: simple-ls2.c
	gcc -o simple-ls2 simple-ls2.c